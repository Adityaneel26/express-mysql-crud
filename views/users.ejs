    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
            <link rel="stylesheet" href="/style.css">

      
    </head>
    <body>
        <table>
            <tr>
                <th>id</th>
                <th>username</th>
                <th>email</th>
            </tr>
            <%  for(user of users){%>
                <tr>
                    <td><%=user.id%></td>
                    <td><%=user.username%></td>
                    <td><%=user.email%></td>
                    <td><form action="/user/<%=user.id%>/edit" method="get"><button>edit</button></form></td>
                    <td><form action="/user/<%=user.id%>/delete?_method=DELETE" method="post"><button>delete</button></form></td>
                </tr>
            <% }%>
        </table>
    </body>
    </html>